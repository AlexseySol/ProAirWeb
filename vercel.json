{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": ".*",
      "dest": "/build/index.html"
    }
  ]
}